@page
@inherits Czar.AbpDemo.Pages.AbpDemoPageBase
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model Czar.AbpDemo.Web.Pages.JobSchedule.CreateModalModel
@{
    Layout = null;
    ViewData["Title"] = "CreateModal";
}

<abp-dynamic-form abp-model="JobInfo" data-ajaxForm="true" asp-page="/JobSchedule/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["New"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>

